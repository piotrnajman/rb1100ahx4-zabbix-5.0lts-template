    <triggers>
        <trigger>
            <expression>({RB1100AHx4:mikrotik4.hddused.min(5m)}/{RB1100AHx4:mikrotik4.hddtotal.last()})*100&gt;{$HDD.UTIL.CRIT}</expression>
            <name>Disk space is critically low (used &gt; {$HDD.UTIL.CRIT}% for 5m)</name>
            <priority>HIGH</priority>
        </trigger>
        <trigger>
            <expression>({RB1100AHx4:mikrotik4.hddused.min(5m)}/{RB1100AHx4:mikrotik4.hddtotal.last()})*100&gt;{$HDD.UTIL.WARN}</expression>
            <name>Disk space is low (used &gt; {$HDD.UTIL.WARN}% for 5m)</name>
            <priority>AVERAGE</priority>
        </trigger>
        <trigger>
            <expression>({RB1100AHx4:mikrotik4.memused.min(5m)}/{RB1100AHx4:mikrotik4.memtotal.last()})*100&gt;{$MEM.UTIL.CRIT}</expression>
            <name>High memory utilization (&gt;{$MEMORY.UTIL.MAX}% for 5m)</name>
            <priority>HIGH</priority>
        </trigger>
    </triggers>
