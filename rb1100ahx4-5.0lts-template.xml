            <macros>
                <macro>
                    <macro>{$CPU.UTIL.CRIT}</macro>
                    <value>90</value>
                </macro>
                <macro>
                    <macro>{$HDD.UTIL.CRIT}</macro>
                    <value>90</value>
                </macro>
                <macro>
                    <macro>{$HDD.UTIL.WAR}</macro>
                    <value>80</value>
                </macro>
                <macro>
                    <macro>{$MEM.UTIL.CRIT}</macro>
                    <value>90</value>
                </macro>
                <macro>
                    <macro>{$PSU.VOLTAGE.MAX}</macro>
                    <value>24.9</value>
                </macro>
                <macro>
                    <macro>{$PSU.VOLTAGE.MIN}</macro>
                    <value>24.0</value>
                </macro>
