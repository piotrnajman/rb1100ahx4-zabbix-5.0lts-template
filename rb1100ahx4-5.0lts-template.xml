                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#IFNAME} discards-in</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.2.2.1.13.{#IFID}</snmp_oid>
                            <key>mikrotik.if.discardsin.[{#IFID}]</key>
