            <discovery_rules>
                <discovery_rule>
                    <name>CPU</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#CPUID},iso.3.6.1.2.1.25.3.2.1.1]</snmp_oid>
                    <key>mikrotik4.cpu.discovery</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Core {#CPUID} Utilisation</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>iso.3.6.1.2.1.25.3.3.1.2.{#CPUID}</snmp_oid>
                            <key>mikrotik4.cpu.util.[{#CPUID}]</key>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>Resources</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{min(5m)}&gt;{$CPU.UTIL.CRIT}</expression>
                                    <name>CPU Core {#CPUID} (over {$CPU.UTIL.CRIT}% for 5m)</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
